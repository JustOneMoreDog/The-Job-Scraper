# Excluding PyCharm Stuff
/.idea
# Making sure not to commit our venv
/virtualenv/*
!/virtualenv/.keep
# Ignoring the chromedriver
/chromedriver/*
!/chromedriver/.keep
# Ignore any cache files
/__pycache__
/scraper_utils/__pycache__
# Ignore all the files created by the job scraper
/customizations_backups/*
!/customizations_backups/.keep
/all_jobs.json
/all_jobs.json.old
/customizations.yaml
# Ignoring job scrape json files
/.job_scrapes/*
!/.job_scrapes/.keep
# Ignoring new log files
/logs/flask/*.log
!/logs/flask/.keep
/logs/scraper/*.log
!/logs/scraper/.keep
/logs/screenshots/*.png
!/logs/screenshots/.keep
/logs/latest/*
!/logs/latest/.keep
/logs/debug_data/*
!/logs/debug_data/.keep
/scrapes/*
!/scrapes/.keep
