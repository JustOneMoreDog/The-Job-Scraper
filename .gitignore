# Excluding PyCharm Stuff
/.idea
# Making sure not to commit our venv
/virtualenv/*
!/virtualenv/.keep
# Ignoring the chromedriver
/chromedriver/*
!/chromedriver/.keep
# Ignore any cache files
/__pycache__
/scraper_utils/__pycache__
/cache/*
/cache/.keep
# Ignore all the files created by the job scraper
/customizations_backups/*
!/customizations_backups/.keep
/scrape_backups/*
!/scrape_backups/.keep
/all_jobs.json
/all_jobs.json.old
# Ignore the logs made by the job scraper and flask
/flask_logs/*
!/flask_logs/.keep
/scraper_logs/*
!/scraper_logs/.keep
# Ignoring all stuff added to templates by the job scraper
/templates/*
# Except for the default templates used for the web gui
!/templates/Welcome Page/
!/templates/base.html
!/templates/customizations.html
!/templates/index.html
!/templates/statistics.html
!/templates/applications.html
/customizations.yaml
# Ignoring job scrape json files
/.job_scrapes/*
!/.job_scrapes/.keep
# Ignoring new log files
/logs/flask/*.log
!/logs/flask/.keep
/logs/scraper/*.log
!/logs/scraper/.keep
/logs/screenshots/*.png
!/logs/screenshots/.keep
/logs/latest/*
!/logs/latest/.keep
/scrapes/*
!/scrapes/.keep
